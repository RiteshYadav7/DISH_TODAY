<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.dishtoday">




    <application
        android:allowBackup="true"
        android:icon="@drawable/download"
        android:label="@string/app_name"
        android:roundIcon="@drawable/download"
        android:preserveLegacyExternalStorage="true"
        android:supportsRtl="true"
        android:theme="@style/Theme.DishToday">
        <activity android:name=".Dashboard"></activity>
        <activity android:name=".Sign_in_activity" />
        <activity android:name=".Signup" />
        <activity android:name=".SplashActivity">


            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".SwipeActivity"></activity>
        <activity android:name=".CategorySubmit"></activity>
        <activity android:name=".Blogger"></activity>
    </application>

</manifest>